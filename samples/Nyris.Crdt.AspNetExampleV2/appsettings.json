{
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http1"
    },
    "Endpoints": {
      "Http": {
        "Protocols": "Http1",
        "Url": "http://0.0.0.0:80"
      },
      "Http2": {
        "Protocols": "Http2",
        "Url": "http://0.0.0.0:8080"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Grpc.AspNetCore": "Information",
      "Microsoft.Extensions.Hosting.Internal": "Warning",
      "Nyris.Crdt.Managed.Services.Hosted.DiscoveryService" : "Warning",
      "Nyris.Crdt.Transport.Grpc" : "Warning",
      "Nyris.Crdt.Managed.Cluster": "Debug",
      "Nyris.Crdt.Managed.Services.Hosted.SynchronizationAndRelocationService": "Debug"
    }
  },
  "AllowedHosts": "*"
}
