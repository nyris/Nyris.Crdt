{
  "Kestrel": {
    "EndpointDefaults": {
      "Protocols": "Http1"
    },
    "Endpoints": {
      "Http": {
        "Protocols": "Http1",
        "Url": "http://localhost"
      },
      "Http2": {
        "Protocols": "Http2",
        "Url": "http://localhost:8080"
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning"
    }
  },
  "Serilog": {
    "Using": [ "Serilog.Sinks.Elasticsearch" ],
    "WriteTo": [
      {
        "Name": "Elasticsearch",
        "Args": {
          "nodeUris": "http://es2.logging:9200",
          "indexFormat": "logstash-matching-{0:yyyy.MM.dd}"
        }
      }
    ],
    "Enrich": [ "FromLogContext", "WithMachineName", "WithProcessId" ],
    "Properties": {
      "Application": "DistributedPrototypeTest"
    },
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning"
      }
    }
  },
  "MongoDb": {
    "connectionString": "mongodb://localhost:27017/nyris",
    "database": "distributed-prototype-test",
    "collection": "images"
  },
  "MetricsOptions": {
    "DefaultContextLabel": "DistributedPrototypeTest",
    "Enabled": true
  },
  "MetricEndpointsOptions": {
    "MetricsEndpointEnabled": true,
    "MetricsTextEndpointEnabled": true,
    "EnvironmentInfoEndpointEnabled": true
  },
  "AllowedHosts": "*",
  "MessageQueue": {
    "Provider": "easynetq",
    "EasyNetQ": {
      "Host": "localhost",
      "Port": 5672,
      "UserName": "guest",
      "Password": "guest",
      "VirtualHost": "/",
      "PublisherConfirms": true
    }
  },
  "Messaging": {
    "in": {
      "imageDataSet": {
        "subscription": {
          "subscriptionId": "nyris.distributedPrototypeTest.image.dataSet",
          "exchange": "nyris.images",
          "routingKey": "image.dataSet",
          "exclusive": false,
          "prefetch": 2,
          "parallelism": 2
        },
        "queue": {
          "activeDeclaration": false,
          "queueBinding": 0,
          "namePrefix": null,
          "durable": true,
          "exclusive": false,
          "autoDelete": false,
          "deadLetterRoutingKey": null
        },
        "exchange": {
          "activeDeclaration": false,
          "durable": true,
          "autoDelete": false
        }
      },
      "imageDeleted": {
        "subscription": {
          "subscriptionId": "nyris.distributedPrototypeTest.image.deleted",
          "exchange": "nyris.images",
          "routingKey": "image.deleted",
          "exclusive": false,
          "prefetch": 2,
          "parallelism": 2
        },
        "queue": {
          "activeDeclaration": false,
          "queueBinding": 0,
          "namePrefix": null,
          "durable": true,
          "exclusive": false,
          "autoDelete": false,
          "deadLetterRoutingKey": null
        },
        "exchange": {
          "activeDeclaration": false,
          "durable": true,
          "autoDelete": false
        }
      }
    }
  }
}
